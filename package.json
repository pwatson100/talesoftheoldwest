{
	"name": "talesoftheoldwest",
	"version": "3.0.0",
	"description": "CSS compiler for the totow system",
	"scripts": {
		"pushLDBtoJSON": "node ./tools/pushLDBtoJSON.mjs",
		"pullJSONtoLDB": "node ./tools/pullJSONtoLDB.mjs",
		"build": "sass src/scss/talesoftheoldwest.scss css/talesoftheoldwest.css --style=expanded --no-source-map",
		"watch": "sass src/scss/talesoftheoldwest.scss css/talesoftheoldwest.css --style=expanded --source-map --watch"
	},
	"browserslist": [
		"last 3 versions"
	],
	"author": "Asacolips",
	"license": "MIT",
	"private": true,
	"devDependencies": {
		"sass": "^1.53.0",
		"@foundryvtt/foundryvtt-cli": "^1.0.2"
	}
}
