{{! Advanced Tab }}
<section class='tab advanced {{tab.cssClass}}' data-group='primary' data-tab='advanced'>
	{{! Editors must receive enriched text data from getData to properly handle rolls, UUID links, etc. }}
	{{#if editable}}
		<prose-mirror name='system.advanced' data-document-u-u-i-d='{{item.uuid}}' value='{{system.advanced}}' toggled='true'>
			{{{enrichedAdvanced}}}
		</prose-mirror>
	{{else}}
		{{{enrichedAdvanced}}}
	{{/if}}
</section>